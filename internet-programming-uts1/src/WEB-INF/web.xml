<web-app>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>Kategori</servlet-name>
        <servlet-class>logic.controllers.KategoriServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Kategori</servlet-name>
        <url-pattern>/kategori</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Hello</servlet-name>
        <servlet-class>logic.controllers.HelloServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
</web-app>