<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ivankara.covid19selfcheck">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Covid19SelfCheck">
        <activity 
            android:name="com.ivankara.covid19selfcheck.ConclusionActivity"
            android:label="@string/conclusion"/>
        <activity
            android:name="com.ivankara.covid19selfcheck.QuestionActivity"
            android:label="@string/question" />
        <activity
            android:name="com.ivankara.covid19selfcheck.MainActivity"
            android:label="Covid19 Self Check by Ivan Kara">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>